    android:paddingTop="7dp"
    android:paddingBottom="7dp"
    app:my_tv_solid="@color/blue_00"
    app:my_tv_corner_radius="4dp"
    android:textColor="@color/white"
    />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="个人信息表"
        android:textColor="@color/gray_33"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:textSize="15sp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表个人信息表"
        android:textColor="@color/gray_66"
        />
    <com.github.customview.MyTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="护照原件模板"
        android:padding="10dp"
        android:layout_marginTop="10dp"
        app:my_tv_solid="@color/white"
        app:my_tv_corner_radius="4dp"
        android:textColor="@color/blue_00"
        app:my_tv_border_color="@color/blue_00"
        app:my_tv_all_line="true"
        />
</LinearLayout>