<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/white"
    >
<include layout="@layout/app_title"/>
     
       <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="match_parent"
           android:orientation="vertical"
           android:background="@color/white"
           android:gravity="center"
           android:paddingBottom="50dp"
           >
           <ImageView
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:src="@drawable/tixian_icon"
               />
           <TextView
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:text="提现申请提交成功"
               android:layout_marginTop="30dp"
               />
           <TextView
               android:layout_width="200dp"
               android:layout_height="wrap_content"
               android:text="客服会在1-2个工作日内，将提现金额打到您的卡内"
               android:layout_marginTop="5dp"
               android:gravity="center"
               />
           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:orientation="horizontal"
               android:gravity="center"
               android:background="@color/white"
               android:layout_marginTop="60dp"
               >
               <com.github.customview.MyTextView
                   android:id="@+id/tv_tixiansuccess_jilu"
                   android:layout_width="90dp"
                   android:layout_height="36dp"
                   android:text="提现记录"
                   android:gravity="center"
                   app:my_tv_corner_radius="5dp"
                   app:my_tv_solid="#DDDDDD"
                   android:textColor="@color/white"
                   android:textSize="14sp"
                   />
               <com.github.customview.MyTextView
                   android:id="@+id/tv_tixiansuccess_back"
                   android:layout_width="90dp"
                   android:layout_height="36dp"
                   android:text="返回"
                   android:gravity="center"
                   app:my_tv_corner_radius="5dp"
                   app:my_tv_solid="@color/blue_00"
                   android:textColor="@color/white"
                   android:textSize="14sp"
                   android:layout_marginLeft="30dp"
                   />
           </LinearLayout>
       </LinearLayout>
</LinearLayout>